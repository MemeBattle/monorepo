{
  "name": "@memebattle/ligretto-core-backend",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "start:dev": "node ace serve --watch --encore-args=\"--port ${LIGRETTO_CORE_BACKEND_PORT}\"",
    "copy:yarn_lock": "cpx ../../yarn.lock build",
    "copy:npm_rc": "cpx ../../.npmrc build",
    "postbuild": "yarn copy:yarn_lock && yarn copy:npm_rc",
    "migrate": "node ace migration:run --force",
    "production-migrate": "node build/ace migration:run --force",
    "ts-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.7.0",
    "@types/lodash": "^4.14.182",
    "cpx": "^1.5.0",
    "pino-pretty": "^7.6.1",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.3",
    "@adonisjs/lucid": "^17.2.0",
    "@adonisjs/repl": "^3.1.11",
    "@memebattle/cas-services": "^3.6.1",
    "@memebattle/ligretto-shared": "^1.0.6",
    "lodash": "^4.17.21",
    "luxon": "^2.0.2",
    "pg": "^8.6.0",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
