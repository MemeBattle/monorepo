{
  "name": "@memebattle/ligretto-core",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "14"
  },
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "start:dev": "node ace serve --watch",
    "copy:yarn_lock": "cpx ../../yarn.lock build",
    "copy:npm_rc": "cpx ../../.npmrc build",
    "postbuild": "yarn copy:yarn_lock && yarn copy:npm_rc",
    "migrate": "node ace migration:run --force"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.0",
    "adonis-preset-ts": "^2.1.0",
    "cpx": "^1.5.0",
    "pino-pretty": "^5.0.1",
    "typescript": "^4.3.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "workspaces": {
    "nohoist": [
      "adonis-preset-ts"
    ]
  },
  "dependencies": {
    "@adonisjs/core": "^5.1.7",
    "@adonisjs/lucid": "^16.1.0",
    "@adonisjs/repl": "^3.1.2",
    "@memebattle/cas-services": "^3.6.1",
    "luxon": "^2.0.2",
    "pg": "^8.6.0",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
