{
  "name": "@memebattle/memebattle",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint-fix:js": "pnpm lint:js --fix",
    "lint:styles": "stylelint {apps,packages}/**/*.scss",
    "lint-fix:styles": "pnpm lint:styles --fix",
    "lint": "pnpm lint:js && pnpm lint:styles",
    "lint-fix": "pnpm lint-fix:js && pnpm lint-fix:styles",
    "test:ci": "pnpm run --parallel --no-bail test:ci",
    "blog:test:ci": "pnpm run --filter @memebattle/blog test:ci",
    "blog:lint": "eslint --ext .js,.jsx,.ts,.tsx apps/blog",
    "ligretto:test:ci": "pnpm run --parallel --no-bail --filter '@memebattle/ligretto-*' test:ci",
    "ligretto:ts-check:ci": "pnpm run --parallel --no-bail --filter '@memebattle/ligretto-*' ts-check",
    "ligretto:start": "pnpm run --parallel --no-bail --filter '@memebattle/ligretto-*' start:dev",
    "ligretto:front:build": "pnpm run --filter @memebattle/ligretto-frontend build",
    "ligretto:front:start:dev": "pnpm run --filter @memebattle/ligretto-frontend start:dev",
    "ligretto:gameplay-backend:start:dev": "pnpm run --filter @memebattle/ligretto-gameplay-backend start:dev",
    "ligretto:gameplay-backend:build": "pnpm run --filter @memebattle/ligretto-gameplay-backend build",
    "ligretto:core-backend:build": "pnpm run --filter @memebattle/ligretto-core-backend build",
    "ligretto:core-backend:migrate": "pnpm run --filter @memebattle/ligretto-core-backend migrate",
    "ligretto:core-backend:start:dev": "pnpm run --filter @memebattle/ligretto-core-backend start:dev",
    "gamehub-client:lint": "eslint --ext .js,.jsx,.ts,.tsx apps/gamehub-client",
    "ligretto-shared:build": "pnpm run --filter @memebattle/ligretto-shared build",
    "ts-check": "pnpm run --parallel --no-bail ts-check",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=24.11",
    "pnpm": ">=10.26.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "catalog:",
    "@storybook/addon-docs": "catalog:",
    "@storybook/addon-essentials": "catalog:",
    "@storybook/addon-links": "catalog:",
    "@storybook/addon-viewport": "catalog:",
    "@storybook/addons": "catalog:",
    "@storybook/preset-scss": "catalog:",
    "@storybook/react": "catalog:",
    "@storybook/react-vite": "catalog:",
    "@types/node": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@typescript/native-preview": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-import": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "eslint-plugin-react": "catalog:",
    "eslint-plugin-react-hooks": "catalog:",
    "eslint-plugin-storybook": "catalog:",
    "prettier": "catalog:",
    "react-docgen-typescript-loader": "catalog:",
    "react-docgen-typescript-plugin": "catalog:",
    "sass": "^1.54.9",
    "storybook": "catalog:",
    "stylelint": "catalog:",
    "stylelint-config-prettier": "catalog:",
    "stylelint-prettier": "catalog:",
    "wait-on": "catalog:"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "pnpm": {
    "overrides": {
      "@types/react": "catalog:",
      "@types/react-dom": "catalog:",
      "@types/node": "catalog:",
      "vite": "npm:rolldown-vite@latest"
    }
  }
}
